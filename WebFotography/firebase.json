{
  "hosting": {
    "public": "dist/web-fotography",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "public/**",
        "destination": "public.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    }
  }
}
